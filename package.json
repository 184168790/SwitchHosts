    "build-w": "webpack -w",
    "pack": "gulp pack",
    "pack-mac": "gulp pack --platform=macOS",
    "pack-win": "gulp pack --platform=win64",
    "zip": "gulp zip",
    "make": "webpack -p && gulp pack && gulp zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oldj/SwitchHosts.git"
  },
  "author": "oldj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oldj/SwitchHosts/issues"
  },
  "homepage": "https://oldj.github.io/SwitchHosts/",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.1",
    "electron": "^1.6.2",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.5.2",
    "gulp-webpack": "^1.5.0",
    "js-beautify": "^1.6.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "nyc": "^10.1.2",
    "spectron": "^3.6.0",
    "style-loader": "^0.13.1",
