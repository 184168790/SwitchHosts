    "url-loader": "^0.5.7",
    "webpack": "^2.3.0",
    "yargs": "^6.5.0"
  },
  "ava": {
    "files": ["src/**/*.ava.js", "test/*.js", "!**/node_modules/**/*.*"],
    "source": ["**/*.{js,jsx}", "!dist/**/*"],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": ["babel-polyfill", "babel-register"],
    "babel": {
      "presets": ["latest", "stage-0"]
    }
  }
