{
  "private": true,
  "scripts": {
    "start": "electron ./build/main.js",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --config scripts/webpack.main.config.js -w",
    "dev:renderer": "APP_ROOT=src/renderer umi dev",
    "version:up": "node ./scripts/version_up.js",
    "build": "npm run version:up && concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config scripts/webpack.main.prod.config.js",
    "build:renderer": "APP_ROOT=src/renderer umi build",
    "make": "node scripts/make.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "electron-store": "^7.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "md5-file": "^5.0.0",
    "mkdirp": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.5.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/mkdirp": "^1.0.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.4.0",
    "antd": "^4.13.1",
    "clsx": "^1.1.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "execa": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "fs-extra": "^9.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^4.2.0",
    "smooth-scroll-into-view-if-needed": "^1.1.30",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.3",
    "umi": "^3.4.0",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "yorkie": "^2.0.0"
  }
}
